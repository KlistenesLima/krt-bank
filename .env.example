# KRT Bank - Environment Variables
# Copy this file to .env and fill in your values

# PostgreSQL
POSTGRES_USER=krt
POSTGRES_PASSWORD=CHANGE_ME_STRONG_PASSWORD
POSTGRES_DB=krtbank

# Keycloak
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=CHANGE_ME_STRONG_PASSWORD

# RabbitMQ
RABBITMQ_DEFAULT_USER=krt
RABBITMQ_DEFAULT_PASS=CHANGE_ME_STRONG_PASSWORD

# API Security
KRT_API_KEY=CHANGE_ME_RANDOM_STRING_64
KRT_ADMIN_API_KEY=CHANGE_ME_RANDOM_STRING_64

# Backblaze B2 (Receipt Storage)
B2_KEY_ID=CHANGE_ME_B2_KEY_ID
B2_APPLICATION_KEY=CHANGE_ME_B2_APPLICATION_KEY

# Grafana Cloud (optional)
GRAFANA_CLOUD_TOKEN=CHANGE_ME_GRAFANA_TOKEN

# CORS
ALLOWED_ORIGINS=https://bank.klisteneslima.dev,https://command.klisteneslima.dev,https://store.klisteneslima.dev

# Environment
ASPNETCORE_ENVIRONMENT=Production
