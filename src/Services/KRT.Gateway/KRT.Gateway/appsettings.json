{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "onboarding-route": {
        "ClusterId": "onboarding-cluster",
        "Match": {
          "Path": "/api/v1/accounts/{**catch-all}"
        }
      },
      "payments-route": {
        "ClusterId": "payments-cluster",
        "Match": {
          "Path": "/api/payments/{**catch-all}"
        }
      },
      "pix-route": {
        "ClusterId": "payments-cluster",
        "Match": {
          "Path": "/api/pix/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "onboarding-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5001/"
          }
        }
      },
      "payments-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5002/"
          }
        }
      }
    }
  }
}
